	<div class="big">
		<img src="img/case-studies/img_01.jpg" alt="Moodboard 1">
	</div>
	<div class="small">
		<img src="img/case-studies/img_02.jpg" alt="Moodboard 2">
		<img src="img/case-studies/img_03.jpg" alt="Moodboard 3">
		<img src="img/case-studies/img_04.jpg" alt="Moodboard 4">
		<img src="img/case-studies/img_05.jpg" alt="Moodboard 5">
	</div>
</div>
<p class="captions">{{__ 'cannalign.stage3.moodboardCaptions'}}</p>


<h3>{{__ 'cannalign.stage3.subHeader2'}}</h3>
