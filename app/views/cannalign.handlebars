{{!-- Intro and overview --}}

<h1>{{__ 'cannalign.openingHeader'}}</h1>
<h2>{{__ 'cannalign.openingSubText'}}</h2>
<p>{{__ 'cannalign.introduction'}}</p>



{{!-- Challenges and roles --}}
{{>challenges-roles}}



{{!-- Stage 1 --}}

<h2>{{__ 'cannalign.stage1.header'}}</h2>
<p>{{__ 'cannalign.stage1.text'}}</p>
<h3>{{__ 'cannalign.stage1.subHeader1'}}</h3>
<p>{{__ 'cannalign.stage1.subHeader1.text'}}</p>


{{!--Cannalign table lists --}}
<div class="cannalign-lists-container">
	<div>
		<h4 class="uppercase">{{__ 'cannalign.table.culture.header'}}</h4>
		<p class="captions">{{__ 'cannalign.table.culture.subHeader'}}</p>
		<ul>
			<li>{{__ 'cannalign.table.culture.accessible'}}</li>
			<li><span class="highlighted">{{__ 'cannalign.table.culture.informationRich'}}</span></li>
			<li>{{__ 'cannalign.table.culture.open'}}</li>
			<li><span class="highlighted">{{__ 'cannalign.table.culture.local'}}</span></li>
		</ul>
	</div>
	<div>
		<h4 class="uppercase">{{__ 'cannalign.table.targetGroup.header'}}</h4>
		<p class="captions">{{__ 'cannalign.table.targetGroup.subHeader'}}</p>
		<ul>
			<li>{{__ 'cannalign.table.targetGroup.young'}}</li>
			<li>{{__ 'cannalign.table.targetGroup.openMinded'}}</li>
			<li><span class="highlighted">{{__ 'cannalign.table.targetGroup.searching'}}</span></li>
			<li>{{__ 'cannalign.table.targetGroup.adaptable'}}</li>
		</ul>
	</div>
	<div>
		<h4 class="uppercase">{{__ 'cannalign.table.voice.header'}}</h4>
		<p class="captions">{{__ 'cannalign.table.voice.subHeader'}}</p>
		<ul>
			<li><span class="highlighted">{{__ 'cannalign.table.voice.trustworthy'}}</span></li>
			<li>{{__ 'cannalign.table.voice.serious'}}</li>
			<li><span class="highlighted">{{__ 'cannalign.table.voice.community'}}</span></li>
			<li><span class="highlighted">{{__ 'cannalign.table.voice.abundant'}}</span></li>
		</ul>
	</div>
	<div>
		<h4 class="uppercase">{{__ 'cannalign.table.feeling.header'}}</h4>
		<p class="captions">{{__ 'cannalign.table.feeling.subHeader'}}</p>
		<ul>
			<li><span class="highlighted">{{__ 'cannalign.table.feeling.happy'}}</span></li>
			<li>{{__ 'cannalign.table.feeling.involved'}}</li>
			<li><span class="highlighted">{{__ 'cannalign.table.feeling.supported'}}</span></li>
		</ul>
	</div>
	<div>
		<h4 class="uppercase">{{__ 'cannalign.table.impact.header'}}</h4>
		<p class="captions">{{__ 'cannalign.table.impact.subHeader'}}</p>
		<ul>
			<li>{{__ 'cannalign.table.impact.expandingHorizons'}}</li>
			<li><span class="highlighted">{{__ 'cannalign.table.impact.extendedCommunity'}}</span></li>
		</ul>
	</div>
</div>
<p class="captions">
	{{{__ 'cannalign.table.captions'}}}
</p>

<h3>{{__ 'cannalign.stage1.subheader2'}}</h3>
<p>{{__ 'cannalign.stage1.subheader2.text'}}</p>


{{!-- Persona --}}
{{>persona}}


<h3>{{__ 'cannalign.stage1.subheader3'}}</h3>
<p>{{__ 'cannalign.stage1.subheader3.text'}}</p>
<img class="case-study-related" src="img/case-studies/img_mindmap.jpg" alt="Stage 1 closing image">
<p class="captions">{{__ 'cannalign.stage1.closingImageCaptions'}}</p>



{{!-- Stage 2 --}}

<h2>{{__ 'cannalign.stage2.header'}}</h2>
<p>{{__ 'cannalign.stage2.text'}}</p>
<img class="case-study-related" src="img/case-studies/img_wireframe.jpg" alt="Stage 2 closing image">
<p class="captions">{{__ 'cannalign.stage2.imageCaptions'}}</p>



{{!-- Stage 3 --}}

<h2>{{__ 'cannalign.stage3.header'}}</h2>
<h3>{{__ 'cannalign.stage3.subheader1'}}</h3>
